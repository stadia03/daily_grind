#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long long n,k;
	    cin>>n>>k;
	    if(n>k){
	        if(n%k==0)
	            cout<<n/k;
	       else
	            cout<<(n/k)+1;
	    }
	    else
	        {
	            if(k%n==0)
	                cout<<k/n;
	           else
	                cout<<(k/n)+1;
	        }
	        cout<<endl;
	}
	return 0;
}
